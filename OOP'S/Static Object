#include<iostream>
using namespace std;

class Example {
public:
    Example() {
        cout <<"Constructor...\n";
    }
    ~Example() {
        cout <<"Distructor...\n";
    }
};

int main() {
    int a = 0;
    if(a == 0) {
        static Example e1;
    }
    cout <<"Code Ending...\n";
    return 0;
}
