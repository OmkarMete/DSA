#include<iostream>
using namespace std;

char getChar(char ch) {
    if(ch == 'z') {
        return 'a';
    } else {
        return ch + 1;
    }
}
int main() {
    char inputChar;
    cout << "Enter a character: ";
    cin >> inputChar;
    cout << "The next character is: " << getChar(inputChar) << endl;
    return 0;
}
