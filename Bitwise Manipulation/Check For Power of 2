#include<iostream>
#include<vector>
using namespace std;

bool ispowerOf2(int num) {
    if(!(num & (num - 1))) {
        return true;
    } else {
        return false;
    }
}
int main() {
    int num = 12;

    if(ispowerOf2(num)) {
        cout << num <<" Is Power Of 2 ";
    } else {
        cout << num <<" Is Not Power Of 2 ";
    }
    return 0;
}
